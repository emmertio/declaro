{
    "extends": "@tsconfig/node20/tsconfig.json",
    "compilerOptions": {
        "strict": false,
        "module": "ESNext",
        "target": "ESNext",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": false,
        "verbatimModuleSyntax": true,
        "sourceMap": true
    }
}
